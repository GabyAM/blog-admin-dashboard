{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "plugin:react/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-unused-vars": "off",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off"
    }
}
